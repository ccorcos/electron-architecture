    "@types/mocha": "whitecolor/mocha-types",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "electron": "^13.1.7",
    "eslint": "^7.31.0",
    "mocha": "^8.4.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  }
